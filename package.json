{
  "name": "@handlewithcare/react-prosemirror",
  "version": "2.4.12",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./src": "./src/index.ts"
  },
  "files": [
    "README.md",
    "LICENSE.txt",
    "dist"
  ],
  "scripts": {
    "build:cjs": "swc ./src --ignore **/__tests__/** -d ./dist/cjs/ --no-swcrc --config-file .swcrc-cjs",
    "build:esm": "swc ./src --ignore **/__tests__/** -d ./dist/esm/",
    "build:types": "tsc --noEmit false --emitDeclarationOnly",
    "build": "concurrently -P 'npm:build:* {@}' --",
    "check:format": "prettier --check .",
    "check:lint": "eslint --cache .",
    "check:types": "tsc",
    "check": "concurrently -P 'npm:check:* {@}' --",
    "clean": "rimraf \"./dist/!(cjs/package.json)\"",
    "demo": "yarn vite",
    "demo:build": "yarn vite build --outDir docs --base=/react-prosemirror/",
    "dev": "yarn build -- --watch",
    "fix:format": "prettier --write .",
    "fix:lint": "eslint --cache --fix .",
    "fix:toc": "markdown-toc --bullets='-' -i README.md",
    "fix": "concurrently -P 'npm:fix:* {@}' --",
    "prepack": "yarn build",
    "test:wdio": "wdio run ./wdio.conf.ts",
    "test:unit": "jest",
    "test": "yarn test:unit && yarn test:wdio"
  },
  "devDependencies": {
    "@codemirror/commands": "^6.8.0",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/state": "^6.5.1",
    "@codemirror/view": "^6.36.2",
    "@handlewithcare/remark-prosemirror": "^0.1.5",
    "@jest/globals": "^29.6.2",
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/css-tree": "^2.3.10",
    "@types/jest": "^27.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-reconciler": "^0.28.9",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@uiw/react-codemirror": "^4.23.7",
    "@vitejs/plugin-react": "^4.3.1",
    "@wdio/browser-runner": "^9.18.1",
    "@wdio/cli": "^9.18.1",
    "@wdio/dot-reporter": "^9.18.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/logger": "^9.18.0",
    "@wdio/mocha-framework": "^9.18.0",
    "@wdio/spec-reporter": "^9.18.0",
    "@wdio/types": "^9.16.2",
    "@yarnpkg/sdks": "^3.0.0-rc.38",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expect": "^29.7.0",
    "expect-webdriverio": "^5.4.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.1.0",
    "markdown-toc": "^1.2.0",
    "mdast-util-gfm-table": "^2.0.0",
    "micromark-extension-gfm-table": "^2.1.1",
    "prettier": "^2.8.3",
    "prosemirror-commands": "^1.5.0",
    "prosemirror-gapcursor": "^1.3.2",
    "prosemirror-history": "^1.4.1",
    "prosemirror-inputrules": "^1.4.0",
    "prosemirror-keymap": "^1.2.1",
    "prosemirror-model": "^1.25.1",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.3",
    "prosemirror-tables": "^1.3.7",
    "prosemirror-test-builder": "^1.1.1",
    "prosemirror-transform": "^1.8.0",
    "prosemirror-view": "1.39.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-reconciler": "^0.32.0",
    "remark-parse": "^11.0.0",
    "rimraf": "^3.0.2",
    "tsx": "^4.20.3",
    "typescript": "^4.9.5",
    "unified": "^11.0.5",
    "vite": "^5.4.5",
    "webdriver": "^9.18.0",
    "webdriverio": "^9.18.1"
  },
  "peerDependencies": {
    "prosemirror-model": "^1.0.0",
    "prosemirror-state": "^1.0.0",
    "prosemirror-view": "1.39.2",
    "react": ">=17 <=19.1.0",
    "react-dom": ">=17 <=19.1.0",
    "react-reconciler": ">=0.26.1 <=0.32.0"
  },
  "packageManager": "yarn@4.6.0",
  "engines": {
    "node": ">=16.9"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
